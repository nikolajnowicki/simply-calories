generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model User {
  id           String  @id @default(cuid())
  username     String  @unique
  passwordHash String
  name         String?
  email        String  @unique
}
